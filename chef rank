import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		int a,x;
		Scanner sc = new Scanner(System.in);
		a = sc.nextInt();
		while(a>0){
		x = sc.nextInt();
        if(x<=10){
            System.out.println("yes");
        }
        else{
            System.out.println("no");
        }
        a = a-1;
		}
	}
}
